{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": [">0.25%"]
        },
        "useBuiltIns": "entry",
        "corejs": { "version": 3, "proposals": true }
      }
    ],
    ["@babel/typescript", { "allowDeclareFields": true }]
  ],
  "plugins": ["@babel/proposal-class-properties", "@babel/proposal-object-rest-spread"]
}
